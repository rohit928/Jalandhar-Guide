<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:paddingLeft="@dimen/activity_horizontal_margin"
    android:paddingRight="@dimen/activity_horizontal_margin"
    android:paddingTop="@dimen/activity_vertical_margin"
    android:paddingBottom="@dimen/activity_vertical_margin"
    android:background="#ffff9671"
>



            <ImageButton

                android:layout_width="120dp"
                android:layout_height="220dp"
                android:id="@+id/imageButton4"
                android:src="@drawable/adming"
                android:onClick="Admin"


                android:layout_alignParentTop="true"
                android:layout_alignRight="@+id/imageButton13"
                android:layout_alignEnd="@+id/imageButton13"
                android:layout_toRightOf="@+id/imageButton"
                android:layout_toEndOf="@+id/imageButton" />

    <Button
                android:layout_width="fill_parent"
                android:layout_height="wrap_content"
                android:text="One Touch Helpline"
                android:id="@+id/button"
                android:onClick="Touch"


                android:layout_gravity="center"
        android:layout_below="@+id/imageButton3"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true"
        android:background="#ff346bff" />




        <ImageButton
            android:layout_width="150dp"
            android:layout_height="220dp"
            android:id="@+id/imageButton"
            android:src="@drawable/tourism"
            android:onClick="Tourism"

            android:layout_gravity="left|top"
            android:layout_alignTop="@+id/imageButton4"
            android:layout_alignParentLeft="true"
            android:layout_alignParentStart="true" />


        <ImageButton
            android:layout_width="150dp"
            android:layout_height="210dp"
            android:id="@+id/imageButton3"
            android:src="@drawable/women_safety"
            android:onClick="Clicks"


            android:layout_gravity="right|top"
            android:background="#ffff9671"
            android:layout_below="@+id/imageButton"
            android:layout_alignParentLeft="true"
            android:layout_alignParentStart="true" />
    <ImageButton
        android:layout_width="150dp"
        android:layout_height="210dp"
        android:id="@+id/imageButton8"
        android:src="@drawable/transport"
        android:onClick="Tourism"
        android:layout_alignTop="@+id/imageButton3"
        android:layout_alignRight="@+id/imageButton4"
        android:layout_alignEnd="@+id/imageButton4"
        android:layout_alignLeft="@+id/imageButton4"
        android:layout_alignStart="@+id/imageButton4" />




    <ImageButton
        android:layout_width="450dp"
        android:layout_height="250dp"
        android:src="@drawable/forecast"
        android:onClick="Logos"
        android:id="@+id/imageButton13"
        android:layout_below="@+id/button"
        android:layout_centerHorizontal="true" />


</RelativeLayout>
