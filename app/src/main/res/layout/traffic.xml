<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent" android:layout_height="match_parent"
    >

    <ImageButton
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/imageButton20"
        android:src="@drawable/back"
        android:onClick="Back7"
        />

    <TextView
        android:layout_width="fill_parent"
        android:layout_height="wrap_content"
        android:textAppearance="?android:attr/textAppearanceLarge"
        android:text="Jalandhar Police"
        android:background="#ff80ff8a"
        android:id="@+id/textView15"
        android:layout_below="@+id/imageButton20"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true" />


    <ImageView
        android:layout_width="70dp"
        android:layout_height="wrap_content"
        android:id="@+id/imageView26"
        android:src="@drawable/call"

        android:layout_below="@+id/textView15"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textAppearance="?android:attr/textAppearanceLarge"
        android:text="1073"

        android:id="@+id/textView41"
        android:layout_alignTop="@+id/imageView26"
        android:layout_alignRight="@+id/imageButton20"
        android:layout_alignEnd="@+id/imageButton20"
        android:layout_marginTop="44dp" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textAppearance="?android:attr/textAppearanceLarge"
        android:text="LandLine"
        android:id="@+id/textView42"
        android:layout_below="@+id/textView41"
        android:layout_toRightOf="@+id/imageView26"
        android:layout_toEndOf="@+id/imageView26" />

    <ImageView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/imageView29"
        android:src="@drawable/top8"
        android:layout_above="@+id/textView15"
        android:layout_toRightOf="@+id/imageButton20"
        android:layout_toEndOf="@+id/imageButton20" />

</RelativeLayout>
