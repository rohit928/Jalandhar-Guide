<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent" android:layout_height="match_parent"
    android:background="#ffff9671">

    <ImageButton
        android:layout_width="190dp"
        android:layout_height="230dp"
        android:id="@+id/imageButton9"
        android:src="@drawable/near"
        android:onClick="Nearby"
        android:layout_marginTop="75dp"
        android:layout_alignParentTop="true"
        android:layout_alignLeft="@+id/imageButton11"
        android:layout_alignStart="@+id/imageButton11" />

    <ImageButton
        android:layout_width="190dp"
        android:layout_height="230dp"
        android:id="@+id/imageButton10"
        android:src="@drawable/interest"
        android:onClick="Places"
        android:layout_alignTop="@+id/imageButton9"
        android:layout_toRightOf="@+id/imageButton9"
        android:layout_marginLeft="20dp"
        android:layout_marginRight="10dp"
        android:layout_toEndOf="@+id/imageButton9" />

    <ImageButton
        android:layout_width="190dp"
        android:layout_height="230dp"
        android:id="@+id/imageButton11"
        android:src="@drawable/about"
        android:layout_marginLeft="10dp"
        android:layout_marginTop="30dp"
        android:layout_below="@+id/imageButton9"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true"
        android:onClick="About"
        />

    <ImageButton
        android:layout_width="200dp"
        android:layout_height="230dp"
        android:id="@+id/imageButton12"
        android:layout_marginRight="10dp"
        android:layout_marginTop="10dp"
        android:layout_marginLeft="20dp"
        android:src="@drawable/reach"
        android:layout_alignTop="@+id/imageButton11"
        android:layout_toRightOf="@+id/imageButton11"
        android:layout_toEndOf="@+id/imageButton11"
        android:layout_alignBottom="@+id/imageButton11" />

    <ImageButton
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/imageButton2"
        android:src="@drawable/back"
        android:onClick="Back"
        android:layout_alignParentTop="true"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true" />

    <ImageView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:src="@drawable/top"
        android:id="@+id/imageView7"
        android:layout_alignBottom="@+id/imageButton2"
        android:layout_toEndOf="@+id/imageButton2"
        android:layout_toRightOf="@+id/imageButton2"
        android:layout_alignParentTop="true" />

</RelativeLayout>
